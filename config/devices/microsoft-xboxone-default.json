{
  "pageStrategy": "html5",
  "modules": {
    "base": "antie/devices/browserdevice",
    "modifiers": [
      "antie/devices/anim/styletopleft",
      "antie/devices/mediaplayer/html5untyped",
      "antie/devices/mediaplayer/live/restartable",
      "antie/devices/data/nativejson",
      "antie/devices/logging/onscreen",
      "antie/devices/logging/xhr",
      "antie/devices/logging/jstestdriver",
      "antie/devices/storage/xboxpls"
    ]
  },
  "logging": {
    "level": "none"
  },
  "displayQuality": "high",
  "streaming": {
    "video": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": ["http"],
          "encodings": ["h264"],
          "transferFormat": ["hls"],
          "maximumVideoLines": 1080
        }
      ]
    },
    "audio": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": ["http"],
          "encodings": ["aac"],
          "maximumBitRate": 192
        }
      ]
    }
  },
  "input": {
    "map": {
      "13": "ENTER",
      "37": "LEFT",
      "38": "UP",
      "39": "RIGHT",
      "40": "DOWN",
      "112": "SUBTITLE",
      "113": "PLAY",
